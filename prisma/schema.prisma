generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Event {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  date           DateTime
  registration   String
  model          String
  pilotInCommand String
  firstOfficer   String
  paxNumber      Int
  departure      String
  destination    String
  arrivalTime    String
  departureTime  String
}
